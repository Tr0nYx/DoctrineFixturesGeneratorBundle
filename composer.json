{
  "name": "webonaute/doctrine-fixtures-generator-bundle",
  "description": "Generate Fixture from your existing data in your database. You can specify the Entity name and the IDs you want to import in your fixture.",
  "type": "symfony-bundle",
  "keywords": [
    "development",
    "database",
    "fixture",
    "symfony",
    "symfony2",
    "symfony4",
    "generator",
    "dumper",
    "doctrine",
    "doctrine2",
    "tests",
    "migration"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Delisle",
      "email": "mdelisle@webonaute.ca"
    }
  ],
  "support": {
    "email": "mdelisle@webonaute.ca",
    "issue": "https://github.com/Webonaute/DoctrineFixturesGeneratorBundle/issues",
    "source": "https://github.com/Webonaute/DoctrineFixturesGeneratorBundle"
  },
  "require": {
    "php": ">=8.2",
    "symfony/framework-bundle": "~5|~6",
    "doctrine/orm": ">=2.2",
    "doctrine/doctrine-bundle": ">=2.7",
    "doctrine/doctrine-fixtures-bundle": ">=2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Webonaute\\DoctrineFixturesGeneratorBundle": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webonaute\\DoctrineFixturesGeneratorBundle\\Tests\\": "tests/"
    }
  },
  "target-dir": "Webonaute/DoctrineFixturesGeneratorBundle",
  "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
